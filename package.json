{
  "name": "education-playground",
  "version": "1.0.0",
  "description": "Education Multimedia Project",
  "main": "index.js",
  "scripts": {
    "postinstall": "typings install",
    "clean": "rm -rf node_modules typings dist",
    "build": "npm run build:dev",
    "build:clean": "rm -rf ./dist && rm -rf ./dist-tmp",
    "build:dev": "npm run server:dev",
    "build:prod": "npm run build:general",
    "build:general": "webpack --config webpack.config.js --display-error-details --progress",
    "build:watch": "npm run build:clean && NODE_ENV=dev webpack --config webpack.config.js --watch --display-error-details --progress",
    "server:watch": "NODE_ENV=dev nodemon --debug ./server.js --watch server.js",
    "start": "npm run build:dev",
    "server:dev": "npm run server:watch & npm run build:watch",
    "start:prod": "npm run build:prod && pm2 startOrRestart ecosystem.json"
  },
  "author": "Will Anderson",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^2.3.0",
    "core-js": "^2.4.1",
    "file-loader": "^0.9.0",
    "material-ui": "^0.16.0",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "restify": "^4.1.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "css-loader": "^0.25.0",
    "css-to-string-loader": "^0.1.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.10.1",
    "path": "^0.12.7",
    "postcss-loader": "^1.0.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.9.1",
    "typescript": "^2.0.3",
    "webpack": "^1.13.2",
    "webpack-livereload-plugin": "^0.9.0",
    "webpack-merge": "^0.14.1"
  }
}
